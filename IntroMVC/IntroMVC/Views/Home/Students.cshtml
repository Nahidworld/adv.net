
@{
    ViewBag.Title = "Students";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}

<h2>Students</h2>

@if (TempData["msg"] != null) { 
    <p class="alert alert-info">@TempData["msg"]</p>
}

<table class="table table-bordered">
    <tr>
        <td>Name</td>
        <td>Id</td>
    </tr>
    @foreach(var item in Model) { 
        <tr>

            <td>@item.Name</td>
            <td>@item.Id</td>
        </tr>
    }
</table>